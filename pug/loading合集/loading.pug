<!DOCTYPE html>
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Document
        link(rel="stylesheet", href="./style.css")                       
    body
        div.g-wrap
            -for(var i = 0; i<4; i++)
                div(class='g-queue g-queue'+(i+1))
                    -for(var j = 0; j<25; j++)
                        div(class='g-item g-item'+(j+1))
                            if j<=1
                                div(class='loading loading'+(i+1))
                                    div(class='shapeWrap shapeWrap'+(i+1))
                                        -for(var h = 0; h<4; h++)
                                            section(class='shape shape'+(h+1)) 
                            else if 2<=j<=18
                                 div(class='c-loading c-loading'+(j+1))
                                        -for(var x = 0; x<9; x++)
                                            div
                            else if 19<=j<=25   
                                -for(var m = 0; m<1; m++)    
                                    div(class='c-loading c-loading'+(j+1))
                                        -for(var y = 0; y<4; y++)
                                            div            
                                                       